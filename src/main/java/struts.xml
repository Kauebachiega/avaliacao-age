<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="registeruser" class="br.com.sistema.soc.action.RegisterAction">
			<result name="REGISTRADO">/Registro.jsp</result>
		</action>
		<action name="report" class="br.com.sistema.soc.action.ReportAction">
			<result name="REPORT">/relatorio.jsp</result>
		</action>
		<action name="updatedetails" class="br.com.sistema.soc.action.UpdateAction">
			<result name="UPDATE">/atualizar.jsp</result>
		</action>
		<action name="deleterecord" class="br.com.sistema.soc.action.DeleteAction">
			<result name="DELETE">/deletar.jsp</result>
		</action>
	</package>
</struts>